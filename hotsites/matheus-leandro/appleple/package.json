	"license": "appleple",
	"devDependencies": {
		"babel-cli": "^6.18.0",
		"babel-preset-es2015": "^6.18.0",
		"node-sass": "^4.3.0",
		"nodemon": "^1.11.0",
		"npm-run-all": "^4.0.1",
		"http-server": "^0.9.0",
		"uglify-js": "^2.7.5"
	}
}
